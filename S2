SELECT cons_sp.OBJECT_NAME    AS CONSUMER_PROFILE,
       sdc.OBJECT_ID          AS DELIV_CHANNEL_ID,
       st.RECEIVING_PROTOCOL
FROM SCI_PROFILE cons_sp
JOIN SCI_DELIV_CHAN sdc
  ON cons_sp.DELIV_CHANNEL_ID = sdc.OBJECT_ID
JOIN SCI_TRANSPORT st
  ON sdc.TRANSPORT_ID = st.OBJECT_ID
WHERE cons_sp.OBJECT_NAME LIKE '%_consumer'
  AND st.RECEIVING_PROTOCOL = 'FTP'
ORDER BY cons_sp.OBJECT_NAME;
