import xml.etree.ElementTree as ET

def parse_to_string(xml_string):
    """
    Cleans an embedded XML string:
    - Parses the string as XML
    - Serializes it back to string without escaping tags like < and >
    """
    try:
        element = ET.fromstring(xml_string)
        return ET.tostring(element, encoding='unicode')
    except ET.ParseError:
        # If string is not well-formed XML, return as-is
        return xml_string


for schedule in matched_schedules:
    if 'SCHEDULE_PARAMS' in schedule:
        schedule['SCHEDULE_PARAMS'] = parse_to_string(schedule['SCHEDULE_PARAMS'])
    if 'SCHEDULE_TIMINGXML' in schedule:
        schedule['SCHEDULE_TIMINGXML'] = parse_to_string(schedule['SCHEDULE_TIMINGXML'])
